<service>
  <id>consul</id>
  <name>Consul</name>
  <description>consul.io Agent</description>
  <executable>consul.exe</executable>
  <arguments>agent -config-dir=<%= node['consul']['config_dir'] %> -data-dir=<%= node['consul']['data_dir'] %></arguments>
  <logmode>rotate</logmode>
  <startmode>delayed-auto</startmode>
  <depend>Winmgmt</depend>
  <depend>Dhcp</depend>
  <depend>Dnscache</depend>
  <depend>Tcpip</depend>
</service>
